<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AppTheme" parent="Theme.MaterialComponents.NoActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorOnPrimary">@color/colorOnBackground</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorOnSurface">@color/colorOnBackground</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="colorSurface">@color/colorPrimary</item>
        <item name="colorSecondary">@color/colorPrimaryDark</item>
        <item name="android:colorBackground">@color/colorPrimaryDark</item>

    </style>

    <style name="RatingBar" parent="Theme.AppCompat">
        <item name="colorControlNormal">@color/grey_soft</item>
        <item name="colorControlActivated">@color/md_green_400</item>
    </style>

    <style name="AppThemeNew" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="windowActionBarOverlay">true</item>
    </style>
</resources>